		CharacterHasStatus(_Character, BurningE)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendo5, INT:-1, _Source)
		CharacterRemoveStatus(_Character, BurningE)
	ELIF "c1"
		CharacterHasStatus(_Character, BurningF)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendo6, INT:-1, _Source)
		CharacterRemoveStatus(_Character, BurningF)
	ELIF "c1"
		CharacterHasStatus(_Character, BurningG)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendo7, INT:-1, _Source)
		CharacterRemoveStatus(_Character, BurningG)
	ELIF "c1"
		CharacterHasStatus(_Character, NECROFIRE)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendo8, INT:-1, _Source)
		CharacterRemoveStatus(_Character, NECROFIRE)
	ELIF "c1"
		CharacterHasStatus(_Character, BurningH)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendo8, INT:-1, _Source)
		CharacterRemoveStatus(_Character, BurningH)
	ELIF "c1"
		CharacterHasStatus(_Character, ChilledA)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoN1, INT:-1, _Source)
		CharacterRemoveStatus(_Character, ChilledA)
	ELIF "c1"
		CharacterHasStatus(_Character, ChilledB)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoN2, INT:-1, _Source)
		CharacterRemoveStatus(_Character, ChilledB)
	ELIF "c1"
		CharacterHasStatus(_Character, ChilledC)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoN3, INT:-1, _Source)
		CharacterRemoveStatus(_Character, ChilledC)
	ELIF "c1|c2|c3|c4|c5|c6"
