EVENT PRETIERArrowApplyStatuses
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	CHARACTER:_CharacterBackup
ON 
	FetchCharacterApplyStatusData(_CharacterBackup, PRETIERArrow)
ACTIONS
	IF "c1"
		IsTagged(_CharacterBackup, "Blue_PRETIERArrow_Tag")
	THEN
		ClearTag(_CharacterBackup, "Blue_PRETIERArrow_Tag")
		//DisplayCombatInfoText(_CharacterBackup, "PRETIERArrow", 5)
		CharacterEvent(_CharacterBackup, BlueRemovePRETIERFailsafeStartTimer)
		IF "c1&c2"
			CharacterHasStatus(_CharacterBackup, PRETIER)
			CharacterGetStatusSourceCharacter(_CharacterBackup, PRETIER, _Source)
		THEN
			SetVar(_CharacterBackup, "LastAttacker", _Source)
		ENDIF
		Set(_Character, _CharacterBackup)
			//
			//DisplayCombatInfoText(_Character, "Characteeeeeer", 5.0)
			//DisplayCombatInfoText(_Source, "Sourceeeeee", 5.0)
			//
		// ~~~~ Elemental Ranger (Elementalist):
		IF "c1&!c2"
			CharacterHasTalent(_Source, TALENT:ElementalRanger)
			IsTagged(_Source, "Blue_CharacterUsedBasicAttack_Tag")
		THEN
			//DisplayCombatInfoText(_Character, "Testest", 5)
			//CLOUD SURFACES:
			//Air:
			IF "c1"
				IsTagged(_Character, "Blue_Elementalist_Air_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Air_Event")
			//Earth:
			ELIF "c1"
				IsTagged(_Character, "Blue_Elementalist_Earth_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Earth_Event")
			//Fire:
			ELIF "c1"
				IsTagged(_Character, "Blue_Elementalist_Fire_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Fire_Event")
			//Physical:
			ELIF "c1"
				IsTagged(_Character, "Blue_Elementalist_Physical_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Physical_Event")
			//Piercing:
			ELIF "c1"
				IsTagged(_Character, "Blue_Elementalist_Piercing_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Piercing_Event")
			//Poison:
			ELIF "c1"
				IsTagged(_Character, "Blue_Elementalist_Poison_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Poison_Event")
			//Water:
			ELIF "c1"
				IsTagged(_Character, "Blue_Elementalist_Water_Tag")
			THEN
				CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Water_Event")
			ENDIF
		ELIF "c1"
			IsTagged(_Source, "Blue_CharacterUsedBasicAttack_Tag")
		THEN
			ClearTag(_Source, "Blue_CharacterUsedBasicAttack_Tag")
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_FireArrow)
		THEN
			//DisplayCombatInfoText(_Character, "Characteeeeeer", 5.0)
			//DisplayCombatInfoText(_Source, "Sourceeeeee", 5.0)
			//
			CharacterApplyStatus(_Character, ApplyBurningC, 5, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER_SCROLL, 5, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER, 0, 0, _Source)
			IF "!c1"
				CharacterHasStatus(_Source, Blue_ArrowheadStance)
			THEN
				CharacterRemoveStatus(_Source, Blue_FireArrow)
				AddTemporaryStatusInfluence(_Source, _Source, Blue_FireArrow, 100)
			ENDIF
			//Set(_Character, _CharacterBackup)
			//
			//DisplayCombatInfoText(_Character, "Characteeeeeer", 5.0)
			//DisplayCombatInfoText(_Source, "Sourceeeeee", 5.0)
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_PinningStance)
		THEN
			CharacterApplyStatus(_Character, ApplyCrippledA, -2, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER, 0, 0, _Source)
			//DisplayCombatInfoText(_Character, "Character", 5.0)
			//DisplayCombatInfoText(_Source, "Source", 5.0)
			//Set(_Character, _CharacterBackup)
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_ExplosiveArrow)
		THEN
			CharacterApplyStatus(_Character, ApplyBurningB, -2, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER_SCROLL, 5, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER, 0, 0, _Source)
			CreateSurfaceAt(_Character, SurfaceExplosionCloud, FLOAT:2, null, _Source)
			//DisplayCombatInfoText(_Character, "Test", 5.0)
			IF "!c1"
				CharacterHasStatus(_Source, Blue_ArrowheadStance)
			THEN
				CharacterRemoveStatus(_Source, Blue_ExplosiveArrow)
				AddTemporaryStatusInfluence(_Source, _Source, Blue_ExplosiveArrow, 100)
			ENDIF
			//Set(_Character, _CharacterBackup)
			//ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoN1, INT:-1, _Source)
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_FreezingArrow)
		THEN
			CharacterApplyStatus(_Character, ApplyChilledC, -2, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER_SCROLL, 5, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER, 0, 0, _Source)
			IF "!c1"
				CharacterHasStatus(_Source, Blue_ArrowheadStance)
			THEN
				CharacterRemoveStatus(_Source, Blue_FreezingArrow)
				AddTemporaryStatusInfluence(_Source, _Source, Blue_FreezingArrow, 100)
			ENDIF
			//Set(_Character, _CharacterBackup)
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_WaterArrow)
		THEN
			CharacterApplyStatus(_Character, WET, 2, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER_SCROLL, 5, 0, _Source)
			CharacterApplyStatus(_Character, POSTIER, 0, 0, _Source)
			CreateSurfaceAt(_Character, SurfaceWater, FLOAT:2, null, _Source)
			IF "!c1"
				CharacterHasStatus(_Source, Blue_ArrowheadStance)
			THEN
				CharacterRemoveStatus(_Source, Blue_WaterArrow)
				AddTemporaryStatusInfluence(_Source, _Source, Blue_WaterArrow, 100)
			ENDIF
			//Set(_Character, _CharacterBackup)
			//Explode goes here
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_BlessedWaterArrow)
		THEN
			CharacterApplyStatus(_Character, Blue_Restoration, 2, 0, _Source)
			//CharacterApplyStatus(_Character, POSTIER, 0, 0, _Source)
			IF "!c1"
				CharacterHasStatus(_Source, Blue_ArrowheadStance)
			THEN
				CharacterRemoveStatus(_Source, Blue_BlessedWaterArrow)
				AddTemporaryStatusInfluence(_Source, _Source, Blue_BlessedWaterArrow, 100)
			ENDIF
			//Set(_Character, _CharacterBackup)
			//Replace heal status
		ENDIF
		IF "c1"
			CharacterHasStatus(_Source, Blue_StaticCloudArrow)
		THEN
			//CharacterApplyStatus(_Character, ApplyShockedB, -2, 0, _Source)
			CreateSurfaceAt(_Character, SurfaceWaterCloudElectrified, FLOAT:1, null, _Source)
			IF "!c1"
				CharacterHasStatus(_Source, Blue_ArrowheadStance)
			THEN
				CharacterRemoveStatus(_Source, Blue_StaticCloudArrow)
