	CHARACTER:_Character
	LIST<STATUS>:_RemoveList
	STATUS:_Result	
ON 
	FetchCharacterApplyStatusData(_Character, Blue_ImpenetrableBarrier)
ACTIONS
	Set(_Result,Blue_ImpenetrableBarrier)
