EVENT BlueRemoveInvisibilityOnWetBlue
VARS
	CHARACTER:_Character
	LIST<STATUS>:_RemoveList
	STATUS:_Result
ON
	FetchCharacterApplyStatusData(_Character, Blue_InvisibleBlue)
ACTIONS
	ListClear(_RemoveList)
	Set(_Result, Blue_InvisibleBlue)
	IF "c1"
		CharacterHasStatus(_Character, WET)
	THEN
		ListAdd(_RemoveList, WET)
	ENDIF
	IF "c1"
		CharacterHasStatus(_Character, Blue_GlitterDust)
	THEN
		ListAdd(_RemoveList, Blue_InvisibleBlue)
		Set(_Result, null)
	ENDIF
	RETURN(_RemoveList,_Result,null)
	
