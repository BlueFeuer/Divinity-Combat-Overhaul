EVENT ColdSnapDealDamage
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_Level
	LIST<STATUS>:_RemoveList
ON
	FetchCharacterApplyStatusData(_Character, Blue_ColdSnap)
ACTIONS
	ListClear(_RemoveList)
	IF "c1"
		IsTagged(_Character, "Blue_ColdSnapDoesStuff")
	THEN
		IF "c1&c2"
			CharacterHasStatus(_Character, PRETIER)
			CharacterGetStatusSourceCharacter(_Character, PRETIER, _Source)
		THEN
			SetVar(_Character, "LastAttacker", _Source)
		ENDIF
		IF "c1"
			GetVar(_Source, _Character, "LastAttacker")
		THEN
		ENDIF
		ListAdd(_RemoveList, ChilledA)
		ListAdd(_RemoveList, ChilledB)
		ListAdd(_RemoveList, ChilledC)
		ListAdd(_RemoveList, ChilledD)
		ListAdd(_RemoveList, ChilledE)
		ListAdd(_RemoveList, ChilledF)
		ListAdd(_RemoveList, ChilledG)
		ListAdd(_RemoveList, ChilledH)
		ListAdd(_RemoveList, FROZEN)
		IF "c1"
			CharacterHasStatus(_Character, ChilledA)
		THEN
			ExplodeAt(_Character, Blue_Explosion_ColdSnap, INT:-1, _Source)
		ELIF "c1|c2"
			CharacterHasStatus(_Character, ChilledB)
			CharacterHasStatus(_Character, ChilledC)
		THEN
			ExplodeAt(_Character, Blue_Explosion_ColdSnapB, INT:-1, _Source)
		ELIF "c1|c2|c3|c4"
			CharacterHasStatus(_Character, ChilledD)
			CharacterHasStatus(_Character, ChilledE)
			CharacterHasStatus(_Character, ChilledF)
			CharacterHasStatus(_Character, ChilledG)
		THEN
			//DisplayCombatInfoText(_Character, "Test", 4)
			ExplodeAt(_Character, Blue_Explosion_ColdSnapC, INT:-1, _Source)
		ELIF "c1"
			CharacterHasStatus(_Character, ChilledH)
		THEN
			ExplodeAt(_Character, Blue_Explosion_ColdSnapD, INT:-1, _Source)
		ELIF "c1"
			CharacterHasStatus(_Character, FROZEN)
		THEN
			ExplodeAt(_Character, Blue_Explosion_ColdSnapE, INT:-1, _Source)
		ENDIF
	ENDIF
	RETURN(_RemoveList,null,INT:2)

//CONDITIONALS
EVENT BlueConditionalFlamingCrescendo
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	LIST<STATUS>:_RemoveList
	INT:_Turns
ON
	FetchCharacterApplyStatusData(_Character, Blue_FlamingCrescendo_Detonate)
ACTIONS
	IF "c1&c2"
		GetStatusTurns(_Character, Blue_FlamingCrescendo, _Turns)
		CharacterGetStatusSourceCharacter(_Character, Blue_FlamingCrescendo, _Source)
	THEN
	ENDIF
	IF "c1&c2"
		IsLessThen(_Turns, INT:3)
		CharacterHasStatus(_Character, BurningA)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoReal, INT:-1, _Source)
		ListAdd(_RemoveList, Blue_FlamingCrescendo)
	ELIF "c1|c2"
		CharacterHasStatus(_Character, BurningB)
		CharacterHasStatus(_Character, BurningC)
	THEN
		IF "c1"
			IsLessThen(_Turns, INT:4)
		THEN
			ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoReal, INT:-1, _Source)
			ListAdd(_RemoveList, Blue_FlamingCrescendo)
		ENDIF
	ELIF "c1|c2|c3|c4"
		CharacterHasStatus(_Character, BurningD)
		CharacterHasStatus(_Character, BurningE)
		CharacterHasStatus(_Character, BurningF)
		CharacterHasStatus(_Character, BurningG)
	THEN
		IF "c1"
			IsLessThen(_Turns, INT:5)
		THEN
			ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoReal, INT:-1, _Source)
			ListAdd(_RemoveList, Blue_FlamingCrescendo)
		ENDIF
	ELIF "c1&c2"
		IsLessThen(_Turns, INT:6)
		CharacterHasStatus(_Character, BurningH)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoReal, INT:-1, _Source)
		ListAdd(_RemoveList, Blue_FlamingCrescendo)
	ELIF "c1"
		IsLessThen(_Turns, INT:2)
	THEN
		ExplodeAt(_Character, Blue_Explosion_FlamingCrescendoReal, INT:-1, _Source)
		ListAdd(_RemoveList, Blue_FlamingCrescendo)
	ENDIF
	RETURN(_RemoveList,null,null)	

