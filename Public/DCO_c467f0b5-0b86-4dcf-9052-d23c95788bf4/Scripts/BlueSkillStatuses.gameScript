EVENT BlueAnticipationApply
VARS
	CHARACTER:_Character
	LIST<STATUS>:_RemoveList
	STATUS:_Result
	FLOAT:_StatCheck
	INT:_StatInt
ON 
	FetchCharacterApplyStatusData(_Character, Blue_Anticipation)
ACTIONS
	ListClear(_RemoveList)
	ListAdd(_RemoveList, Blue_Anticipation)
	IF "c1"
		CharacterGetStat(_StatCheck, _Character, ActionPoints)
	THEN
	ENDIF
	Cast(_StatInt, _StatCheck)
	IF "c1"
		CharacterGetWeaponRange(_, _StatCheck, _Character)
	THEN
		Add(_StatCheck, FLOAT:1.0)
	ENDIF
	SetTag(_Character, "Blue_CharacterIsAnticipating")
	IF "c1"
		IsLessThen(_StatCheck, FLOAT:2)
	THEN
		//DisplayCombatInfoText(_Character, "1", 5)
		IF "c1"
			IsEqual(_StatInt, INT:1)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C1_R1)
		ELIF "c1"
			IsEqual(_StatInt, INT:2)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C2_R1)
