EVENT BlueSkillHeartseeker
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_StatCheck
	FLOAT:_FloatHelp
	INT:_Index
ON 
	OnCharacterStatusApplied(_Character, Blue_HeartSeeker)
ACTIONS
	IF "c1&c2"
		CharacterHasStatus(_Character, PRETIER)
		CharacterGetStatusSourceCharacter(_Character, PRETIER, _Source)
	THEN
		SetVar(_Character, "LastAttacker", _Source)
	ENDIF
	IF "c1&c2&c3"
		GetVar(_Source, _Character, "LastAttacker")
		CharacterGetStat(_StatCheck, _Character, Vitality)
		CharacterGetStat(_FloatHelp, _Source, Vitality)
	THEN
		Subtract(_FloatHelp, _StatCheck)
		IF "c1"
			CharacterGetStat(_StatCheck, _Source, VitalityMax)
		THEN
			Multiply(_FloatHelp, _StatCheck)
		ENDIF
	ENDIF
	IF "c1"
		CharacterIsDead(_Character)
	THEN
		Multiply(_FloatHelp, FLOAT:1.2)
	ENDIF
	IF "c1"
		IsGreaterThen(_FloatHelp, FLOAT:0)
	THEN
		SetVar(_Source, "DamageToDeal", _FloatHelp)
		CharacterCharacterEvent(_Source, _Source, "Blue_Damage_Piercing_Event")
	ENDIF
	
EVENT BlueTerribleFlameApply
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_StatCheck
ON 
	OnCharacterStatusApplied(_Character, Blue_TerribleFlame)
ACTIONS
	IF "c1&c2"
		CharacterHasStatus(_Character, PRETIER)
		CharacterGetStatusSourceCharacter(_Character, PRETIER, _Source)
	THEN
		SetVar(_Character, "LastAttacker", _Source)
	ENDIF
	IF "c1"
		GetVar(_Source, _Character, "LastAttacker")
	THEN
	ENDIF
	IF "c1"
		CharacterGetStat(_StatCheck, _Character, "Vitality")
	THEN
	ENDIF
	IF "c1"
		IsLessThen(_StatCheck, FLOAT:0.125)
	THEN
		CharacterApplyStatus(_Character, ApplyFearF) 
	ELIF "c1"
		IsLessThen(_StatCheck, FLOAT:0.25)
	THEN
		CharacterApplyStatus(_Character, ApplyFearE) 
	ELIF "c1"
		IsLessThen(_StatCheck, FLOAT:0.375)
	THEN
		CharacterApplyStatus(_Character, ApplyFearD) 
	ELIF "c1"
		IsLessThen(_StatCheck, FLOAT:0.5)
	THEN
		CharacterApplyStatus(_Character, ApplyFearC) 
	ENDIF
	IF "c1"
		IsLessThen(_StatCheck, FLOAT:0.5)
	THEN
		CreateSurfaceAt(_Character, SurfaceFireCursed, FLOAT:2, INT:2, _Source)
	ENDIF
	
