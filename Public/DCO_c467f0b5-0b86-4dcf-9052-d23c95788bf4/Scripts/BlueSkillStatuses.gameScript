	ELIF "!c1"
		IsLessThen(_StatCheck, FLOAT:5)
	THEN
		//DisplayCombatInfoText(_Character, "5", 5)
		IF "c1"
			IsEqual(_StatInt, INT:1)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C1_R5)
		ELIF "c1"
			IsEqual(_StatInt, INT:2)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C2_R5)
		ELIF "c1"
			IsEqual(_StatInt, INT:3)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C3_R5)
		ELIF "c1"
			IsEqual(_StatInt, INT:4)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C4_R5)
		ELIF "c1"
			IsGreaterThen(_StatInt, INT:4)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C5_R5)
		ENDIF
	ENDIF
	CharacterUseActionPoints(_Character, _StatInt)
	RETURN(_RemoveList,_Result,INT:1)

EVENT BlueSandCoffinPercentageDamage
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_DamageToDeal
	FLOAT:_PercentDamage
	//FIXEDSTRING:_Text
	//STRING:_String
ON 
	OnCharacterStatusApplied(_Character, Blue_SandCoffin)
ACTIONS
	IF "c1&c2"
		CharacterHasStatus(_Character, PRETIER)
		CharacterGetStatusSourceCharacter(_Character, PRETIER, _Source)
	THEN
		SetVar(_Character, "LastAttacker", _Source)
	ENDIF
	IF "c1"
		GetVar(_Source, _Character, "LastAttacker")
	THEN
	ENDIF
	IF "c1&c2"
		CharacterGetStat(_DamageToDeal, _Character, Vitality)
		CharacterGetStat(_PercentDamage, _Source, Intelligence)
	THEN
		IF "c1"
			IsGreaterThen(_PercentDamage, FLOAT:40)
		THEN
			Set(_PercentDamage, FLOAT:40)
		ENDIF
		Multiply(_PercentDamage, FLOAT:0.01)
		/*
		Cast(_VitalityChangeAsInt, _VitalityChange)
		Print(_String, "Took [1]% of maximum health as bonus damage.", _VitalityChangeAsInt)
		Cast(_Text, _String)
		DisplayCombatInfoText(_Character, _Text, FLOAT:0)
		Subtract(_CurrHealth, _VitalityChange)
		SetHealth(_Character, _CurrHealth)
		*/
		Multiply(_DamageToDeal, _PercentDamage)
		SetVar(_Character, "DamageToDeal", _DamageToDeal)
		CharacterCharacterEvent(_Character, _Source, "Blue_Damage_Physical_Event")
	ENDIF

