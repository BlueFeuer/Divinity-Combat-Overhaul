EVENT Blue_CharacterGainsSourcePoint
VARS
	CHARACTER:_Character
	INT:_AbilCheck
ON
	OnCharacterFlagSet("GLO_SPUnlockAvailable", _Character)
ACTIONS
	IF "c1"
		IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
	THEN
		Set(_AbilCheck, INT:3)
	ENDIF
	IF "c1"
		IsTagged(_Character, "Blue_SourceMaximum_2_Tag")
	THEN
		Set(_AbilCheck, INT:2)
	ENDIF
	IF "c1"
		IsTagged(_Character, "Blue_SourceMaximum_1_Tag")
	THEN
		Set(_AbilCheck, INT:1)
	ENDIF
	Add(_AbilCheck, INT:1)
	IF "!c1"
		IsLessThen(_AbilCheck, INT:3)
	THEN
		IF "!c1"
			IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
		THEN
			//DisplayCombatInfoText(_Character, "Source max 3", 5)
			SetTag(_Character, "Blue_SourceMaximum_3_Tag")
			ClearTag(_Character, "Blue_SourceMaximum_2_Tag")
			ClearTag(_Character, "Blue_SourceMaximum_1_Tag")
		ENDIF
	ELIF "!c1"
		IsLessThen(_AbilCheck, INT:2)
	THEN
		IF "!c1&!c2"
			IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
			IsTagged(_Character, "Blue_SourceMaximum_2_Tag")
		THEN
			//DisplayCombatInfoText(_Character, "Source max 2", 5)
			SetTag(_Character, "Blue_SourceMaximum_2_Tag")
			ClearTag(_Character, "Blue_SourceMaximum_1_Tag")
		ENDIF
	ELIF "!c1"
		IsLessThen(_AbilCheck, INT:1)
	THEN
		IF "!c1&!c2&!c3"
			IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
			IsTagged(_Character, "Blue_SourceMaximum_2_Tag")
			IsTagged(_Character, "Blue_SourceMaximum_1_Tag")
		THEN
			//DisplayCombatInfoText(_Character, "Source max 1", 5)
			SetTag(_Character, "Blue_SourceMaximum_1_Tag")
		ENDIF
	ENDIF


// Give the player the ability to empower abilities when they acquire Bless.
EVENT PlayerGiveThemTheEmpower
VARS
	CHARACTER:_Character
ON
	OnSkillAdded(_Character, Target_Bless, _)
ACTIONS
	IF "!c1"
		CharacterHasSkill(_Character, Blue_Shout_SourceEmpower)
	THEN
		CharacterAddSkill(_Character, Blue_Shout_SourceEmpower)
		CharacterCharacterEvent(_Character, _Character, "Blue_SourceMastery_Show")
		SetTag(_Character, "Blue_SourceMastery_Revealed")
	ENDIF
	
EVENT Blue_OnCriticalHit
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_StatCheck
ON 
	OnCriticalHit(_Source, _, _Character, _)
ACTIONS
	//DisplayCombatInfoText(_Character, "Critical", 5)
	IF "c1"
		CharacterHasTalent(_Source, Torturer)
	THEN
		CharacterApplyStatus(_Source, Blue_Advantage_Remove, 0)
		IF "c1"
			GetVar(_StatCheck, _Character, "AptitudeBonus")
		THEN
			Add(_StatCheck, FLOAT:1.0)
			SetVar(_Character, "AptitudeBonus", FLOAT:_StatCheck)
			//One of this function's purpose is to reset the AptitudeBonus and there's no downside to triggering it here.
			//This is in case a character's weapon doesn't apply a tiered status so it doesn't linger.
			CharacterEvent(_Character, "BlueRemovePRETIERFailsafeStartTimer")
		ENDIF
	ENDIF
	IF "c1"
		CharacterHasTalent(_Source, ViolentMagic)
	THEN
		IF "!c1&!c2&!c3&!c4&!c5&!c6&!c7&!c8"
			CharacterHasStatus(_Character, Blue_Flow)
			CharacterHasStatus(_Character, Blue_Frost)
			CharacterHasStatus(_Character, Blue_Seared)
			CharacterHasStatus(_Character, Blue_Toxicity_1)
			CharacterHasStatus(_Character, Blue_Toxicity_2)
			CharacterHasStatus(_Character, Blue_Toxicity_3)
			CharacterHasStatus(_Character, Blue_Toxicity_4)
			CharacterHasStatus(_Character, Blue_Toxicity_5)
		THEN
			IF "c1"
				IsTagged(_Character, "Blue_MalevolentMojo_Tag")
			THEN
				ClearTag(_Character, "Blue_MalevolentMojo_Tag")
			ENDIF
		ENDIF
		IF "c1"
			IsTagged(_Character, "Blue_Damage_Fire_Tag")
		THEN
			IF "!c1"
				IsTagged(_Character, "Blue_MalevolentMojo_Tag")
			THEN
				SetTag(_Character, "Blue_MalevolentMojo_Tag")
				CharacterApplyStatus(_Character, Blue_Seared, 2)
			ENDIF
		ELIF "c1"
			IsTagged(_Character, "Blue_Damage_Air_Tag")
		THEN
			IF "!c1"
				IsTagged(_Character, "Blue_MalevolentMojo_Tag")
			THEN
				SetTag(_Character, "Blue_MalevolentMojo_Tag")
				CharacterApplyStatus(_Character, Blue_Flow, 2)
			ENDIF
		ELIF "c1"
			IsTagged(_Character, "Blue_Damage_Water_Tag")
		THEN
			IF "!c1"
				IsTagged(_Character, "Blue_MalevolentMojo_Tag")
			THEN
				SetTag(_Character, "Blue_MalevolentMojo_Tag")
				CharacterApplyStatus(_Character, Blue_Frost, 2)
			ENDIF
		ELIF "c1"
			IsTagged(_Character, "Blue_Damage_Poison_Tag")
		THEN
			
			IF "c1"
				CharacterHasStatus(_Character, Blue_Toxicity_1)
			THEN
				CharacterApplyStatus(_Character, Blue_Toxicity_2, 2)
			ELIF "c1"
				CharacterHasStatus(_Character, Blue_Toxicity_2)
			THEN
				CharacterApplyStatus(_Character, Blue_Toxicity_3, 2)
			ELIF "c1"
				CharacterHasStatus(_Character, Blue_Toxicity_3)
			THEN
				CharacterApplyStatus(_Character, Blue_Toxicity_4, 2)
			ELIF "c1|c2"
				CharacterHasStatus(_Character, Blue_Toxicity_4)
				CharacterHasStatus(_Character, Blue_Toxicity_5)
			THEN
				CharacterApplyStatus(_Character, Blue_Toxicity_5, 2)
			ELSE
				IF "!c1"
					IsTagged(_Character, "Blue_MalevolentMojo_Tag")
				THEN
					SetTag(_Character, "Blue_MalevolentMojo_Tag")
					CharacterApplyStatus(_Character, Blue_Toxicity_1, 2)
				ENDIF
			ENDIF
			
		ENDIF
	ENDIF
	IF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_1, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_1)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_2, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_2)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_3, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_3)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_4, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_4)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_5, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_5)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_6, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_6)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_7, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_7)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_8, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_8)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_9, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Cruelty_9)
	THEN
		CharacterApplyStatus(_Character, Blue_Cruelty_10, 1)
	ENDIF

EVENT Blue_CharacterIncapacitatedPeriodicCheck
VARS
	CHARACTER:_Character
	CHARACTER:_AoO
ON 
	OnCharacterEvent(_Character, "CharacterIncapacitatedPeriodicCheck")
ACTIONS
	IF "c1&c2&!c3"
		CharacterHasStatus(_Character, Blue_GuidingWindsSource)
		GetVar(_AoO, _Character, "AttackOfOpportunity")
		IsEqual(_AoO, null)
	THEN
		CharacterRemoveStatus(_Character, Blue_GuidingWindsSource)
	ENDIF
	
EVENT Blue_Crucify
VARS
	CHARACTER:_Character
	CHARACTER:_Source
ON 
	OnCharacterStatusAttempt(_Character, Blue_Crucify)
ACTIONS
	IF "c1&c2"
		CharacterHasStatus(_Character, PRETIER)
		CharacterGetStatusSourceCharacter(_Character, PRETIER, _Source)
	THEN
		SetVar(_Character, "LastAttacker", _Source)
	ENDIF
	IF "c1"
		GetVar(_Source, _Character, "LastAttacker")
	THEN
	ENDIF
	ExplodeAt(_Character, Blue_Explosion_Crucify_Enemy, INT:-1, _Source)
	ExplodeAt(_Character, Blue_Explosion_Crucify_Ally, INT:-1, _Source)
	
EVENT Blue_WalkedOnIce
VARS
	CHARACTER:_Character
	INT:_RandomInt
ON 
	OnCharacterStatusAttempt(_Character, Blue_WalkedOnIce)
ACTIONS
	IF "c1"
		CharacterHasStatus(_Character, Blue_WalkingOnIce)
	THEN
		CharacterApplyStatus(_Character, PRETIERCLEAR, -2)
		GetRandomBetween(_RandomInt, INT:1, INT:50)
		//1-16 = A, 17-24 = B, 25-32 = C, 33-36 = D, 37-40 = E, 41-44 = F, 45-48 = G, 49-50 = H
		IF "!c1"
			IsGreaterThen(_RandomInt, INT:16)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredA, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:24)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredB, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:32)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredC, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:36)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredD, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:40)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredE, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:44)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredF, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:48)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredG, -2)
		ELIF "!c1"
			IsGreaterThen(_RandomInt, INT:50)
		THEN
			CharacterApplyStatus(_Character, ApplyStaggeredH, -2)
		ENDIF
		CharacterApplyStatus(_Character, POSTIER, 0)
	ENDIF

// SPOIDER
EVENT SPOIDERS
VARS
	CHARACTER:_Character
	CHARACTER:_Spawned
	FLOAT:_StatCheck
	INT:_HelperInt
ON
	OnCharacterStatusRemoved(_Character, Blue_Infested)
ACTIONS
	IF "c1"
		CharacterGetStat(_StatCheck, _Character, Level)
	THEN
		Cast(_HelperInt, _StatCheck)
		SpawnCharacter(_Spawned, 2e0c1f1f-645a-449b-b559-38d3fa88c3d9, _Character, INT:1, INT:0, null, _HelperInt)
		CharacterSetTemporaryHostileRelation(_Spawned, _Character)
	ENDIF

