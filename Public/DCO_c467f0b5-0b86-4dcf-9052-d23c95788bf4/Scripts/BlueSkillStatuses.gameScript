			IsEqual(_StatInt, INT:4)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C4_R1)
		ELIF "c1"
			IsGreaterThen(_StatInt, INT:4)
		THEN
			Set(_Result, STATUS:Blue_Anticipation_C5_R1)
