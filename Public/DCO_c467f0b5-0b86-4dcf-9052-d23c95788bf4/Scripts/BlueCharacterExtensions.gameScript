EVENT Blue_ArbiterCycle7_Misc
VARS
	CHARACTER:_Character
ON
	OnCharacterEvent(_Character, Blue_ArbiterCycle7_Misc)
ACTIONS
	//Check ignored tag
	IF "c1"
		IsTagged(_Character, "AI_IGNORED_TARGET")
	THEN
		//DisplayCombatInfoText(_Character, "Ignored", 5)
		IF "!c1&!c2&!c3&!c4&!c5"
			CharacterHasStatus(_Character, Blue_InvisibleOrange)
			CharacterHasStatus(_Character, Blue_InvisibleBlue)
			CharacterHasStatus(_Character, Blue_InvisibleWhite)
			CharacterHasStatus(_Character, SNEAKING)
			CharacterHasStatus(_Character, Blue_InvisibleBlind)
		THEN
			ClearTag(_Character, "AI_IGNORED_TARGET")
		ENDIF
	ENDIF


// This just ensures abilities are corrected, as it's a pretty janky setup that
// only checks the total points spent and not individual abilities, and the
// total may not have changed even if the distribution has when they exit the mirror.
EVENT PlayerResetAbilitiesOnCharCreationStopped
VARS
	CHARACTER:_Character
ON
	OnCharacterCreationStopped(_Character)
ACTIONS
	IF "c1&!c2"
		CharacterIsInParty(_Character)
		CharacterIsSummon(_Character)
	THEN
		SetVar(_Character, "CurrTotalAbilityPoints", INT:-666)
	ENDIF

