EVENT BlueCharacterDies
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	FLOAT:_Distance
ON
	OnDie(_Character, _, _Source, _)
ACTIONS
	IF "c1&c2"
		CharacterHasStatus(_Source, Blue_DeathWish)
		CharacterIsEnemy(_Source, _Character)
	THEN
		SetTag(_Source, "IgnoreDeathWishRemoval")
		CharacterRemoveStatus(_Source, STATUS:Blue_DeathWish)
		CharacterApplyStatus(_Source, STATUS:Blue_DeathWishFulfilled, -2)
	ENDIF
	IF "c1&!c2"
		CharacterHasTalent(_Source, Executioner)
		IsTagged(_Source, "Blue_Talent_Executioner_Tag")
	THEN
		SetTag(_Source, "Blue_Talent_Executioner_Tag")
		IF "c1&c2"
			GetDistance(_Distance, _Source, _Character)
			IsGreaterThen(_Distance, FLOAT:5.0)
		THEN
			CharacterApplyStatus(_Source, STATUS:Blue_Talent_Executioner_Far)
			StatusText(_Character, "Executed! +1 AP, +10% Vitality restored.")
		ELSE
			CharacterApplyStatus(_Source, STATUS:Blue_Talent_Executioner_Close)
			StatusText(_Character, "Executed! +2 AP, +20% Vitality restored.")
		ENDIF
	ENDIF

EVENT BlueTurnStartedEvent
VARS
	CHARACTER:_Character
	CHARACTER:_Source
	INT:_TurnsLeft
ON
	OnCharacterEvent(_Character, "BlueTurnStartedEvent")
ACTIONS
	SetVar(_Character, "CurrPosition", FLOAT3:{-2;-2;-2})
	SetVar(_Character, "DamageDealtThisTurn", FLOAT:0)
	SetVar(_Character, "AptitudeBonus", FLOAT:1)
	CharacterApplyStatus(_Character, Blue_CheckTurnEnd, -2)
	CharacterApplyStatus(_Character, Blue_ShieldsUpRemove)
	CharacterEvent(_Character, "GlobalBlacksmithingCorrecter")
	CharacterEvent(_Character, "GlobalStatsSanityCorrecter")
	SetTag(_Character, "Blue_ForceEnableArbiter")
	IF "c1"
		IsTagged(_Character, "Blue_CharacterIsAnticipating")
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_Anticipation_Remove, 0)
		ClearTag(_Character, "Blue_CharacterIsAnticipating")
	ENDIF
	IF "c1"
		IsTagged(_Character, "Blue_Talent_Executioner_Tag")
	THEN
		ClearTag(_Character, "Blue_Talent_Executioner_Tag")
	ENDIF
	IF "c1"
		CharacterHasStatus(_Character, Blue_InvisibleWhite)
	THEN
		CharacterRemoveStatus(_Character, Blue_InvisibleWhite)
	ENDIF
	IF "c1"
		CharacterHasStatus(_Character, Blue_Talent_DRUNK)
	THEN
		CharacterRemoveStatus(_Character, Blue_Talent_DRUNK)
	ENDIF
	IF "c1"
		CharacterHasStatus(_Character, Blue_Talent_DieHard)
	THEN
		CharacterRemoveStatus(_Character, Blue_Talent_DieHard)
	ENDIF

	
EVENT BlueExitCombatSetTag
VARS
	CHARACTER:_Character
ON
	OnLeftCombat(_Character, _)
ACTIONS
	CharacterRemoveStatus(_Character, Blue_Status_ArmorRestore_Debuff)
	CharacterRemoveStatus(_Character, Blue_Status_ArmorRestore)
	CharacterRemoveStatus(_Character, Blue_AICalculationStats_Other)
	IF "c1"
		CharacterHasStatus(_Character, Blue_Talent_Ghastly_SP)
	THEN
		CharacterRemoveStatus(_Character, Blue_Talent_Ghastly_SP)
	ENDIF
	ClearTag(_Character, "Blue_Ghastly_Round_4")
	ClearTag(_Character, "Blue_Ghastly_Round_3")
	ClearTag(_Character, "Blue_Ghastly_Round_2")
	ClearTag(_Character, "Blue_Ghastly_Round_1")
