EVENT Blue_SourceFountain_Used
VARS
	CHARACTER:_Character
	INT:_AbilCheck
ON
	OnCharacterEvent(_Character, Blue_SourceFountain_Used)
ACTIONS
	IF "c1&c2&!c3&!c4"
		CharacterGetSourcePoints(_Character, _AbilCheck)
		IsEqual(_AbilCheck, INT:0)
		CharacterHasStatus(_Character, SOURCE_MUTED)
		IsTagged(_Character, "Blue_Empowering_RemoveOnly")
	THEN
		CharacterApplyStatus(_Character, Blue_Source_Cursed, 30)
		CharacterAddSourcePoints(_Character, INT:1)
	ENDIF
	
