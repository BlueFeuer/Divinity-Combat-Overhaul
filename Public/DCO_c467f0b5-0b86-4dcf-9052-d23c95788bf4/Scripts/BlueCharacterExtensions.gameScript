EVENT SourcePointCorrecter
VARS
	CHARACTER:_Character
	INT:_IntVar
	INT:_CurrSource
ON
	OnCharacterEvent(_Character, "SourcePointCorrecter")
ACTIONS
	IF "c1"
		CharacterGetSourcePoints(_Character, _CurrSource)
	THEN
		IF "c1"
			GetVar(_IntVar, _Character, "HelperInt")
		THEN
		ENDIF
		IF "c1"
			IsLessThen(_IntVar, _CurrSource)
		THEN
			IF "c1"
				CharacterHasStatus(_Character, Blue_Source_Cursed)
			THEN
				SetTag(_Character, "Blue_Source_Cursed_Ignore")
				CharacterRemoveStatus(_Character, Blue_Source_Cursed)
			ENDIF
		ENDIF
		IF "!c1"
			IsLessThen(_CurrSource, INT:3)
		THEN
			IF "!c1"
				IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
			THEN
				//DisplayCombatInfoText(_Character, "Source max 3", 5)
				SetTag(_Character, "Blue_SourceMaximum_3_Tag")
				ClearTag(_Character, "Blue_SourceMaximum_2_Tag")
				ClearTag(_Character, "Blue_SourceMaximum_1_Tag")
			ENDIF
		ELIF "!c1"
			IsLessThen(_CurrSource, INT:2)
		THEN
			IF "!c1&!c2"
				IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
				IsTagged(_Character, "Blue_SourceMaximum_2_Tag")
			THEN
				//DisplayCombatInfoText(_Character, "Source max 2", 5)
				SetTag(_Character, "Blue_SourceMaximum_2_Tag")
				ClearTag(_Character, "Blue_SourceMaximum_1_Tag")
			ENDIF
		ELIF "!c1"
			IsLessThen(_CurrSource, INT:1)
		THEN
			IF "!c1&!c2&!c3"
				IsTagged(_Character, "Blue_SourceMaximum_3_Tag")
				IsTagged(_Character, "Blue_SourceMaximum_2_Tag")
				IsTagged(_Character, "Blue_SourceMaximum_1_Tag")
			THEN
				//DisplayCombatInfoText(_Character, "Source max 1", 5)
				SetTag(_Character, "Blue_SourceMaximum_1_Tag")
			ENDIF
		ENDIF
		IF "c1&!c2"
			CharacterHasTalent(_Character, Stench)
			CharacterInCreation(_Character)
		THEN
			IF "!c1"
				IsTagged(_Character, "Blue_Ghastly_Applied")
			THEN
				SetTag(_Character, "Blue_Ghastly_Applied")
			ENDIF
			IF "!c1"
				//Less than or equal to
				IsGreaterThen(_CurrSource, INT:0)
			THEN
				CharacterApplyStatus(_Character, Blue_Talent_Ghastly_0, -1)
			ELIF "c1"
				IsEqual(_CurrSource, INT:1)
			THEN
				CharacterApplyStatus(_Character, Blue_Talent_Ghastly_1, -1)
			ELIF "c1"
				IsEqual(_CurrSource, INT:2)
			THEN
				CharacterApplyStatus(_Character, Blue_Talent_Ghastly_2, -1)
			ELIF "!c1"
				//Greater than or equal to
				IsLessThen(_CurrSource, INT:3)
			THEN
				CharacterApplyStatus(_Character, Blue_Talent_Ghastly_3, -1)
			ENDIF
		ELIF "c1"
			IsTagged(_Character, "Blue_Ghastly_Applied")
		THEN
			ClearTag(_Character, "Blue_Ghastly_Applied")
			CharacterApplyStatus(_Character, Blue_Talent_Ghastly_Remove, 0)
		ENDIF
	ENDIF

