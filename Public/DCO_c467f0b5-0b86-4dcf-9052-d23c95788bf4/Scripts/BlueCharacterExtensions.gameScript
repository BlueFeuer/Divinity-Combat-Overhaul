EVENT ResistanceCorrecterFire
VARS
	CHARACTER:_Character
	FLOAT:_Resistance
	FLOAT:_ResistanceCap
ON
	OnCharacterEvent(_Character, "ResistanceCorrecterFire")
ACTIONS
	IF "!c1&!c2&!c3&!c4&!c5&!c6&!c7&!c8"
		CharacterHasStatus(_Character, FireDebuff128)
		CharacterHasStatus(_Character, FireDebuff64)
		CharacterHasStatus(_Character, FireDebuff32)
		CharacterHasStatus(_Character, FireDebuff16)
		CharacterHasStatus(_Character, FireDebuff8)
		CharacterHasStatus(_Character, FireDebuff4)
		CharacterHasStatus(_Character, FireDebuff2)
		CharacterHasStatus(_Character, FireDebuff1)
	THEN
		IF "c1&c2"
			GetVar(_ResistanceCap, _Character, "ResistanceCap")
			CharacterGetStat(_Resistance, _Character, FireResistance)
		THEN
			Subtract(_Resistance, _ResistanceCap)
			Add(_Resistance, FLOAT:1)
			IF "c1"
				IsGreaterThen(_Resistance, FLOAT:256)
			THEN
				AddStatusInfluence(_Character, FireDebuff256, 100)
				Subtract(_Resistance, FLOAT:256)
			ENDIF
			IF "c1"
				IsGreaterThen(_Resistance, FLOAT:128)
			THEN
				AddStatusInfluence(_Character, FireDebuff128, 100)
				Subtract(_Resistance, FLOAT:128)
			ENDIF
			IF "c1"
				IsGreaterThen(_Resistance, FLOAT:64)
			THEN
				AddStatusInfluence(_Character, FireDebuff64, 100)
				Subtract(_Resistance, FLOAT:64)
