EVENT BlueCharacterMissed
VARS
	CHARACTER:_Source
	CHARACTER:_Character
	FLOAT:_Distance
ON
	OnMiss(_Source, _, _Character, _)
ACTIONS
	//If playing on hardcore, missing a character causes them to look towards you.
	IF "c1"
		IsHardcore()
	THEN
		SetVar(_Character, "LookAtChar", CHARACTER:_Source)
	ENDIF
	//Parry Master
	IF "c1"
		CharacterHasTalent(_Character, DualWieldingDodging)
	THEN
		IF "c1&!c2&!c3&c4&c5&!c6&!c7&!c8&!c9&!c10&!c11&!c12&!c13&!c14&!c15&!c16&!c17&!c18&!c19&!c20&!c21&!c22&c23"
			CharacterInWeaponRange(_Character, _Source) //c1, If _Source is within weapon range of _Character
			IsTagged(_Character, "Blue_CharacterIsDisabled") //c2
			IsEqual(_Character, _Source) //c3
			CharacterIsEnemy(_Character, _Source) //c4
			GetDistance(_Distance, _Source, _Character) //c5
			IsGreaterThen(_Distance, FLOAT:5.0) //c6
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp1) //c7
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp2) //c8
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp3) //c9
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp4) //c10
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp5) //c11
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp6) //c12
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp7) //c13
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp8) //c14
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp9) //c15
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp10) //c16
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp11) //c17
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp12) //c18
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp13) //c19
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp14) //c20
			CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp15) //c21
			IsInActiveTurn(_Character) //c22
			IsInCombat(_Character) //c23
		THEN
			SetVar(_Character, "AttackOfOpportunity", CHARACTER:_Source)
		ENDIF
	ENDIF
	//Shields Up
	IF "c1"
		CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp1)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_ShieldsUp_Remove, 0)
	ELIF "c1"
		CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp2)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_ShieldsUp_Remove, 0)
	ELIF "c1"
		CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp3)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_ShieldsUp1, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp4)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_ShieldsUp2, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp5)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_ShieldsUp3, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, STATUS:Blue_ShieldsUp6)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_ShieldsUp4, 1)
	ELIF "c1"
