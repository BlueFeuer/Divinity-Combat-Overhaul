EVENT LevelCorrecter
VARS
	CHARACTER:_Character
	FLOAT:_AbilCheck
ON
	OnCharacterEvent(_Character, LevelCorrecter)
ACTIONS
	IF "c1"
		CharacterGetStat(_AbilCheck, _Character, Level)
	THEN
		IF "c1"
			IsGreaterThen(_AbilCheck, FLOAT:0)
		THEN
			AddStatusInfluence(_Character, STATUS:LevelUpPersuasion1, 100)
		ENDIF
		IF "c1"
			IsGreaterThen(_AbilCheck, FLOAT:3)
		THEN
			AddStatusInfluence(_Character, STATUS:LevelUpPersuasion2, 100)
		ENDIF
		IF "c1"
			IsGreaterThen(_AbilCheck, FLOAT:7)
		THEN
			AddStatusInfluence(_Character, STATUS:LevelUpPersuasion3, 100)
		ENDIF
		IF "c1"
			IsGreaterThen(_AbilCheck, FLOAT:11)
		THEN
			AddStatusInfluence(_Character, STATUS:LevelUpPersuasion4, 100)
		ENDIF
		IF "c1"
			IsGreaterThen(_AbilCheck, FLOAT:15)
		THEN
			AddStatusInfluence(_Character, STATUS:LevelUpPersuasion5, 100)
		ENDIF
	ENDIF

EVENT BlueCharacterEntersCombatEvent
VARS
	CHARACTER:_Character
	INT:_WealBuffBinaryDecoder
	INT:_AbilCheck
	INT:_RepeatTimes
	FLOAT:_RepeatChance
ON
	OnCharacterEvent(_Character, "BlueCharacterEntersCombatEvent")
ACTIONS
	IF "!c1&!c2&c3&!c4"
		CharacterIsInParty(_Character)
		CharacterIsSummon(_Character)
		CharacterHasSkill(_Character, Shout_RecoverArmour)
		CharacterHasSkill(_Character, Shout_RecoverArmour_AI)
	THEN
		CharacterAddSkill(_Character, Shout_RecoverArmour_AI)
	ENDIF
	IF "c1"
		IsTagged(_Character, "Blue_CharacterIsAnticipating")
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_Anticipation_Remove, 0)
		ClearTag(_Character, "Blue_CharacterIsAnticipating")
	ENDIF
	IF "c1&!c2"
		CharacterIsInParty(_Character)
		CharacterIsSummon(_Character)
	THEN
		//Clearing cursed source
		IF "c1"
			CharacterHasStatus(_Character, Blue_Source_Cursed)
		THEN
			CharacterRemoveStatus(_Character, Blue_Source_Cursed)
		ENDIF
		//Weal Setup
		SetVar(_Character, "WealBuffBinaryTurnA", INT:0)
		SetVar(_Character, "WealBuffBinaryTurnD", INT:0)
		ClearTag(_Character, "Blue_Eff1Used")
		ClearTag(_Character, "Blue_Eff2Used")
		ClearTag(_Character, "Blue_Eff3Used")
		ClearTag(_Character, "Blue_Eff4Used")
		ClearTag(_Character, "Blue_Eff5Used")
		ClearTag(_Character, "Blue_Eff6Used")
		//ClearTag(_Character, "Blue_Eff7Used")
		ClearTag(_Character, "Blue_Eff8Used")
		ClearTag(_Character, "Blue_Eff9Used")
		ClearTag(_Character, "Blue_Eff10Used")
		ClearTag(_Character, "Blue_Eff11Used")
		ClearTag(_Character, "Blue_Eff12Used")
		ClearTag(_Character, "Blue_Eff13Used")
		ClearTag(_Character, "Blue_Eff14Used")
		ClearTag(_Character, "Blue_Eff15Used")
		ClearTag(_Character, "Blue_Eff16Used")
		//ClearTag(_Character, "Blue_Eff17Used")
		//ClearTag(_Character, "Blue_Eff18Used")
		//ClearTag(_Character, "Blue_Eff19Used")
		//Removing effects I didn't like/end up using from the pool.
		SetTag(_Character, "Blue_Eff7Used")
		SetTag(_Character, "Blue_Eff17Used")
		SetTag(_Character, "Blue_Eff18Used")
		SetTag(_Character, "Blue_Eff19Used")
		IF "c1"
			CharacterGetAbility(_AbilCheck, _Character, TwoHanded)
		THEN
			IF "c1"
				IsGreaterThen(_AbilCheck, INT:20)
			THEN
				Set(_AbilCheck, INT:20)
			ENDIF
		ENDIF
		IF "c1"
			IsGreaterThen(_AbilCheck, INT:0)
