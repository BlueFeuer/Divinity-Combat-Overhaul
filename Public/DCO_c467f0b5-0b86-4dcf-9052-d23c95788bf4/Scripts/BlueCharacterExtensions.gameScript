		CharacterHasStatus(_Character, Blue_InvisibleBlind)
	THEN
		CharacterRemoveStatus(_Character, Blue_InvisibleBlind)
	ENDIF
	ClearTag(_Character, "BlueCheckStatusesThisTurn")


EVENT Blue_AddArmor
VARS
	CHARACTER:_Character
	FLOAT:_StatCheck
	FLOAT:_FloatHelp
ON 
	OnCharacterEvent(_Character, "Blue_AddArmor")
ACTIONS
	IF "!c1&!c2"
		CharacterIsInParty(_Character)
		CharacterIsSummon(_Character)
	THEN
		IF "c1&c2"
			CharacterGetStat(_StatCheck, _Character, PhysicalArmorPoints)
			CharacterGetStat(_FloatHelp, _Character, Level)
		THEN
			Add(_FloatHelp, FLOAT:-4)
			Multiply(_FloatHelp, _StatCheck)
			Multiply(_FloatHelp, 0.18)
		ENDIF
		IF "c1"
			IsGreaterThen(_FloatHelp, FLOAT:0)
		THEN
			SetVar(_Character, "DamageToDeal", _FloatHelp)
			CharacterCharacterEvent(_Character, _Character, "Blue_Damage_AddArmor_Event")
		ENDIF
	ELSE
		CharacterApplyStatus(_Character, Blue_Status_AddArmor_Remove, 0)
	ENDIF

