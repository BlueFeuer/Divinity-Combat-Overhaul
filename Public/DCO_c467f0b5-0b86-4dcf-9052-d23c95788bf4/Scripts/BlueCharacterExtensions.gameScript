	CHARACTER:_Character
	INT:_HelperInt
	FLOAT3:_Position
	SURFACE:_SurfaceCache
	//STRING:_Str
	//FIXEDSTRING:_Text
ON
	OnCharacterEvent(_Character, BlueCheckElementalist)
ACTIONS
	//Elementalist
	IF "!c1&c2"
	//IF "!c1&c2&c3"
		CharacterInCreation(_Character)
		//GetPosition(_Character, _Position)
		CharacterHasTalent(_Character, TALENT:ElementalRanger)
	THEN
		IF "c1"
			GetVar(_SurfaceCache, _Character, "SurfaceCache")
		THEN
		ENDIF
		//Print(_Str, "Surface: [1], Position: [2]", _SurfaceCache, _Position)
		//Cast(_Text, _Str)
		//DisplayCombatInfoText(_Character, _Text, FLOAT:5)
		IF "c1|!c2"
			IsEqual(SURFACE:_SurfaceCache, SURFACE:SurfaceNone)
			FindSurface(_Position, _Character, FLOAT:0, FLOAT:0.5, _SurfaceCache)
		THEN
			//DisplayCombatInfoText(_Character, "false", FLOAT:5)
			IF "c1"
				FindSurface(_Position, _Character, FLOAT:0, FLOAT:0.5, SURFACE:SurfaceFire)
			THEN
				Set(_SurfaceCache, SURFACE:SurfaceFire)
				Set(_HelperInt, INT:1)
			ELIF "c1"
				FindSurface(_Position, _Character, FLOAT:0, FLOAT:0.5, SURFACE:SurfaceFireBlessed)
			THEN
				Set(_SurfaceCache, SURFACE:SurfaceFireBlessed)
				Set(_HelperInt, INT:1)
			ELIF "c1"
				FindSurface(_Position, _Character, FLOAT:0, FLOAT:0.5, SURFACE:SurfaceFireCursed)
			THEN
				Set(_SurfaceCache, SURFACE:SurfaceFireCursed)
				Set(_HelperInt, INT:1)
			ELIF "c1"
				FindSurface(_Position, _Character, FLOAT:0, FLOAT:0.5, SURFACE:SurfaceLava)
			THEN
				Set(_SurfaceCache, SURFACE:SurfaceLava)
				Set(_HelperInt, INT:1)
			ELIF "c1"
				FindSurface(_Position, _Character, FLOAT:0, FLOAT:0.5, SURFACE:SurfaceBloodElectrified)
			THEN
				Set(_SurfaceCache, SURFACE:SurfaceBloodElectrified)
				Set(_HelperInt, INT:4)
			ELIF "c1"
