EVENT Blue_Event_GetAnticipation
VARS
	CHARACTER:_Character
	CHARACTER:_Defender
ON
	OnCharacterCharacterEvent(_Character, _Defender, "Blue_Event_GetAnticipation")
ACTIONS
	//_Defender is defender, _Character is attacker
	SetVar(_Character, "AttackOfOpportunity", CHARACTER:_Defender)
	SetTag(_Character, "Blue_CharacterAnticipated")
	//R1
	IF "c1"
		CharacterHasStatus(_Character, Blue_Anticipation_C1_R1)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_Anticipation_Remove, 0)
		ClearTag(_Character, "Blue_CharacterIsAnticipating")
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Anticipation_C2_R1)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_Anticipation_C1_R1, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Anticipation_C3_R1)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_Anticipation_C2_R1, 1)
	ELIF "c1"
		CharacterHasStatus(_Character, Blue_Anticipation_C4_R1)
	THEN
		CharacterApplyStatus(_Character, STATUS:Blue_Anticipation_C3_R1, 1)
