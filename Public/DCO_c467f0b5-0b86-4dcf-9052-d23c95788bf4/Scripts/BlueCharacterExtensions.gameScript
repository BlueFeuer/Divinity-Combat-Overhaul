ACTIONS
	IF "c1"
		GetVar(_HelperVar, _Character, "HelperVar")
	THEN
	ENDIF
	IF "!c1"
		IsLessThen(_HelperVar, INT:16)
	THEN
		SetTag(_Character, "Blue_LivingArmor_Air_5")
		Subtract(_HelperVar, INT:16)
	ENDIF
	IF "!c1"
		IsLessThen(_HelperVar, INT:8)
	THEN
