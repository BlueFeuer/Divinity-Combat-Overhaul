		IF "c1"
			CharacterGetAbility(_AbilCheck, _Character, Barter)
		THEN
			//Cast(_ConvertFrom, _AbilCheck)
			//Multiply(_ConvertFrom, FLOAT:15) //Grit
			IF "c1"
				IsGreaterThen(_AbilCheck, INT:15)
			THEN
				Set(_AbilCheck, INT:15)
			ENDIF
