			Cast(_StatCheck, _AbilCheck)
			Subtract(_Strength, _StatCheck)
			Set(_StatCheck, _Strength)
			Cast(_Strength, _AbilCheck)
			IF "c1"
				IsRandom(_StatCheck)
			THEN
				Add(_Strength, 1)
			ENDIF
		ENDIF
