	THEN
		//Cast(_ConvertFrom, _AbilCheck)
		//Multiply(_ConvertFrom, FLOAT:15) //Willpower
		IF "c1"
			IsGreaterThen(_AbilCheck, INT:15)
		THEN
			Set(_AbilCheck, INT:15)
		ENDIF
		// Precalculated values.
		GetElement(_ConvertFrom, _AbilCheck, 0.0, 25.0, 44.0, 58.0, 68.0, 76.0, 82.0, 87.0, 90.0, 92.0, 94.0, 96.0, 97.0, 98.0, 98.0, 99.0, 99.0)
		Cast(_AptitudePercent, _ConvertFrom)
		Multiply(_ConvertFrom, FLOAT:0.01)
		Subtract(_ConvertFrom, FLOAT:1)
		Multiply(_ConvertFrom, FLOAT:-1)
