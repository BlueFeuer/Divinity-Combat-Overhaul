EVENTS

EVENT Blue_Resistances_Poison
VARS
	CHARACTER:_Character
	INT:_AbilCheck
	FLOAT:_Resistance
	FLOAT:_StatCheck
ON
	OnCharacterEvent(_Character, Blue_Resistances_Poison)
ACTIONS
	IF "c1"
		GetVar(_Resistance, _Character, "ResistanceCap")
	THEN
		IF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_2)
		THEN
			Add(_Resistance, FLOAT:1)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_3)
		THEN
			Add(_Resistance, FLOAT:1)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_4)
		THEN
			Add(_Resistance, FLOAT:2)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_5)
		THEN
			Add(_Resistance, FLOAT:2)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_6)
		THEN
			Add(_Resistance, FLOAT:3)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_7)
		THEN
			Add(_Resistance, FLOAT:3)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_8)
		THEN
			Add(_Resistance, FLOAT:4)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_9)
		THEN
			Add(_Resistance, FLOAT:4)
		ELIF "c1"
			CharacterHasStatus(_Character, Blue_Weaponry_Shield_10)
		THEN
			Add(_Resistance, FLOAT:5)
		ENDIF
		IF "c1&c2"
			CharacterGetAbility(_AbilCheck, _Character, Perseverance)
			CharacterHasTalent(_Character, TALENT:IceKing)
		THEN
			Divide(_AbilCheck, INT:2)
			Cast(_StatCheck, _AbilCheck)
			Add(_Resistance, _StatCheck)
		ENDIF
		IF "c1"
			CharacterGetStat(_StatCheck, _Character, Constitution)
		THEN
			IF "!c1"
				IsLessThen(_StatCheck, FLOAT:35)
			THEN
				Add(_Resistance, FLOAT:5)
			ELIF "!c1"
				IsLessThen(_StatCheck, FLOAT:28)
			THEN
				Add(_Resistance, FLOAT:4)
			ELIF "!c1"
				IsLessThen(_StatCheck, FLOAT:22)
			THEN
				Add(_Resistance, FLOAT:3)
			ELIF "!c1"
				IsLessThen(_StatCheck, FLOAT:17)
			THEN
				Add(_Resistance, FLOAT:2)
			ELIF "!c1"
				IsLessThen(_StatCheck, FLOAT:13)
			THEN
				Add(_Resistance, FLOAT:1)
			ENDIF
		ENDIF
		SetVar(_Character, "ResistanceCap", _Resistance)
	ENDIF

