	THEN
		Cast(_AbilCheck, _Strength)
		Subtract(_ConvertFrom, FLOAT:10)
		Multiply(_ConvertFrom, FLOAT:5) //APTITUDE
		Cast(_AptitudePercent, _ConvertFrom)
		Multiply(_ConvertFrom, FLOAT:0.01)
		Add(_ConvertFrom, FLOAT:1)
		Multiply(_Strength, _ConvertFrom)
