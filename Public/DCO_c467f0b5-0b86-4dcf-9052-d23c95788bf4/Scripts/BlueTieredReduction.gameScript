// Wait - Why is adding armor in with the damage events?
EVENT Blue_Damage_AddArmor_Event
VARS
	CHARACTER:_Source //Source of damage
	CHARACTER:_Character
	INT:_AbilCheck
	FLOAT:_Damage
	FLOAT:_FloatHelp
ON
	OnCharacterCharacterEvent(_Character, _Source, "Blue_Damage_AddArmor_Event")
ACTIONS
	IF "c1&c2"
		CharacterGetStat(_Damage, _Source, Level)
		GetVar(_FloatHelp, _Character, "DamageToDeal")
	THEN
		Cast(_AbilCheck, _Damage)
	ENDIF
	Set(_Damage, 0.3)
	IF "c1"
		IsLessThen(_AbilCheck, INT:21)
	THEN
		GetElement(_Damage, INT:_AbilCheck,7.69,7.69,6.67,5.88,5.26,5,4.55,4.17,4,3.85,3.57,3.45,3.23,3.13,3.03,2.86,2.78,2.7,2.63,2.5,2.44)
	ENDIF
	IF "!c1&c2"
		IsLessThen(_AbilCheck, INT:21)
		IsLessThen(_AbilCheck, INT:35)
